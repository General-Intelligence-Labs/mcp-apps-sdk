{
  "name": "mcp-apps-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "SDK for building MCP-powered applications with OpenAI Apps",
  "homepage": "https://github.com/general-intelligence-labs/mcp-apps-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/general-intelligence-labs/mcp-apps-sdk.git"
  },
  "license": "MIT",
  "author": "General Intelligence Labs",
  "scripts": {
    "build": "pnpm run -r build",
    "dev": "pnpm run --parallel dev",
    "test": "pnpm run -r test",
    "lint": "pnpm run -r lint",
    "type-check": "pnpm run -r type-check",
    "clean": "pnpm run -r clean && rm -rf node_modules",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  }
}